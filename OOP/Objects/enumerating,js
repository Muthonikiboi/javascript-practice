for(const key of keys){
    console.log(key);
}
//ouput
//radius
//draw

const person = {
    name: "John Doe",
    age: 30,
    job: "Software Engineer",
  };
  
  // Enumerate the properties of the person object using the for...in loop
  for (const property in person) {
    console.log(property);
  }
  
  // Output:
  // name
  // age
  // job
  
  